// recursion
#include<stdio.h>
#include<conio.h>
int fun1(int a,int b);
int fun1(int a,int b);
void main()

{
	printf("%d\n%d\n",fun1(3,8),fun2(3,8)); 
}
int fun1(int a,int b){
	if(a>b)
	return 0;
	else
	return b+fun1(a,b-1);
	
}
int fun2(int a,int b){
	if(a>b)
	return 0;
	else
	return a+fun1(a+1,b);
	
}
